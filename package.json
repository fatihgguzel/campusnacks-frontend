{
  "name": "campusnacks-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-dotenv && craco start",
    "build": "react-dotenv && craco build",
    "test": "craco test",
    "eject": "craco eject",
    "types": "node ./scripts/pull-types.js",
    "icons": "node ./scripts/generate-icons.js",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.cjs",
    "lint:fix": "eslint --fix .",
    "test:build": "craco test --watchAll=false"
  },
  "react-dotenv": {
    "whitelist": [
      "ENV_NAME",
      "BE_BASE_URL"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.5",
    "@reduxjs/toolkit": "1.9.7",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.83",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "@types/react-phone-number-input": "3.1.37",
    "@types/react-transition-group": "4.4.10",
    "axios": "1.6.8",
    "case-anything": "2.1.13",
    "i18next": "23.11.1",
    "izitoast": "1.4.0",
    "jwt-decode": "4.0.0",
    "moment": "2.30.1",
    "react": "18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-dotenv": "0.1.3",
    "react-i18next": "14.1.0",
    "react-phone-number-input": "3.4.1",
    "react-redux": "8.1.3",
    "react-router-dom": "6.22.3",
    "react-scripts": "5.0.1",
    "react-transition-group": "4.4.5",
    "redux-persist": "6.0.0",
    "reselect": "5.1.0",
    "svgson": "5.3.1",
    "typescript": "4.9.5",
    "uuid": "9.0.1",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@craco/craco": "7.1.0",
    "@types/uuid": "9.0.8",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "prettier": "3.2.5"
  }
}
